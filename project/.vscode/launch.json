{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Dashboard (Debug)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
        },
        {
            "name": "Dashboard (Profile)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
        },
        {
            "name": "Dashboard (Release)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
        },
        {
        "name": "Flutter Web Release on port 80",
        "type": "dart",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "program": "lib/main.dart",
        "flutterMode": "release",
        "toolArgs": ["-d", "web-server","--web-hostname", "0.0.0.0","--web-port", "80"]
        }
    ]
}